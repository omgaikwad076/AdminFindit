<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <stroke
        android:width="1dp"
        android:color="#2F7E2F"/> <!-- Green color for the border -->

    <size
        android:width="100dp"
        android:height="25dp"/> <!-- Define the size of the square -->

    <solid
        android:color="@android:color/transparent"/> <!-- Transparent fill -->

    <corners
        android:radius="16dp"/> <!-- Corner radius -->

</shape>




